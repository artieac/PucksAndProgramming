	<link href="/Content/Normalize.css" rel="stylesheet" type="text/css" />
	<link href="/Content/WebFlow.css" rel="stylesheet" type="text/css" />
	<link href="/Content/MainPage.css" rel="stylesheet" type="text/css" />
	@RenderSection("headerPlugins", false)
</head>
<body ng-app="theApp">
	@RenderBody()
	<div class="section grey">
		<div class="footer">
			<div class="w-container">
				<div class="row">
					<div class="col-md-4">
						<h5>about PucksAndProgramming</h5>
						<p>Pucks And Programming is the personal site of Arthur Correa.</p>
					</div>
					<div class="col-md-4">
						<h5>useful links</h5>
						<a href="http://pucksandprogramming.com" class="footer-link">PucksAndProgramming.com</a>
						<a href="http://blog.alwaysmoveforward.com" class="footer-link">Blog</a>
						<a href="http://technologyradar.pucksandprogramming.com" class="footer-link">Your Technology Radar</a>
					</div>
					<div class="col-md-4">
						<h5>social</h5>
						<div class="footer-link-wrapper w-clearfix">
							<img src="/Content/images/social-09.svg" width="20" alt="" class="info-icon">
							<a href="https://www.linkedin.com/in/arthur-correa-00864a1" class="footer-link with-icon">Linked In</a>
						</div>
						<div class="footer-link-wrapper w-clearfix">
							<img src="/Content/images/social-18.svg" width="20" alt="" class="info-icon">
							<a href="https://twitter.com/artieac" class="footer-link with-icon">Twitter</a>
						</div>
						<div class="footer-link-wrapper w-clearfix">
							<img src="/Content/images/social-11.svg" width="20" alt="" class="info-icon">
							<a href="https://www.pinterest.com/artieac/" class="footer-link with-icon">Pinterest</a>
						</div>
						<div class="footer-link-wrapper w-clearfix">
							<img src="/Content/images/social-07.svg" width="20" alt="" class="info-icon">
							<a href="https://www.instagram.com/artiecorrea/" class="footer-link with-icon">Instagram</a>
						</div>
					</div>
				</div>
			</div>
		</div>
	</div>
</body>
</html>

